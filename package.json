{
  "name": "basketball",
  "version": "0.2.0",
  "private": true,
  "devDependencies": {
    "husky": "^1.3.1",
    "lint-staged": "^13.2.3",
    "prettier": "^1.18.2",
    "react-scripts": "^5.0.1"
  },
  "engines": {
    "node": "10.15.1"
  },
  "dependencies": {
    "auth0-js": "^9.11.3",
    "bootstrap": "^3.3.7",
    "history": "^5.3.0",
    "react": "^16.11.0",
    "react-bootstrap": "^0.32.4",
    "react-dom": "^16.11.0",
    "react-redux": "^7.1.1",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "start": "SET NODE_OPTIONS=--openssl-legacy-provider && react-scripts start",
    "build": "SET NODE_OPTIONS=--openssl-legacy-provider && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "prettier": "prettier --write \"**/*.js\"",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run prettier",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
